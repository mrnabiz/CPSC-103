#include<stdio.h>
#include<math.h>
main()
{
	int a,b,c,d,e;
	float x1,x2,x3;
	scanf("%d %d %d",&a,&b,&c);
	d=(b*b)-(4*a*c);
	if(d<0) printf("Risheii Vojood Nadarad");
	else
	if(d==0) {x3=((-b)/2*a);
	printf("Risheye Mozaaf=%f",x3);}
	else
	{x1=(-b+sqrt(d))/(2*a);
	x2=(-b-sqrt(d))/(2*a);
	printf("root1= %f \n root2=%f",x1,x2);}
	scanf("%d,&e");
}
